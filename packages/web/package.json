    "test": "jest",
    "test:watch": "jest --watch",
    "ts-proto": "rm -rf ./src/Simulator/SimulateOnServer/Wire && mkdir ./src/Simulator/SimulateOnServer/Wire && protoc --plugin=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=src/Simulator/SimulateOnServer/Wire --ts_proto_opt=oneof=unions --ts_proto_opt=esModuleInterop=true --proto_path=../simulator-rust/src/lib/wire ../simulator-rust/src/lib/wire/*.proto"
